unction handleResize(){\n\t\t\t// Update camera\n\t\t\tcamera.aspect = window.innerWidth / window.innerHeight;\n\t\t\tcamera.updateProjectionMatrix();\n\t\t\t// Update renderer\n\t\t    renderer.setSize(window.innerWidth, window.innerHeight);\n\t\t}\n\n        return () => {\n\t\t\t//remove event listeners\n\t\t\t//window.removeEventListener(\"mousemove\", shaderAnimate);\n\t\t\twindow.removeEventListener('scroll',handleResize);\n\t\t\twindow.removeEventListener('resize',handleResize);\n\t\t\t//remove canvas from tree\n\t\t\tdocument.body.removeChild(renderer.domElement);\t\n\t\t\t//remove gpu assets\n\t\t\tgeometry.dispose();\n\t\t\tmaterial.dispose();\n\t\t\trenderer.dispose();\n\t\t\tscene.remove(mesh);\n\t\t\tcancelAnimationFrame(animate);\n\t\t}\n      },[]);\n\nreturn(\n   null\n)\n\n}\n\n\n\n","/Users/bernardoagbranco/react-threejs/info-page-1/src/components/Info/Info.js",[],["177","178"],"/Users/bernardoagbranco/react-threejs/info-page-1/src/components/Info1/Info.js",[],"/Users/bernardoagbranco/react-threejs/info-page-1/src/components/Info2/Info.js",[],"/Users/bernardoagbranco/react-threejs/info-page-1/src/components/Info3/Info.js",[],"/Users/bernardoagbranco/react-threejs/info-page-1/src/components/Gallery/Gallery.js",[],"/Users/bernardoagbranco/react-threejs/info-page-2/src/index.js",[],["179","180"],"/Users/bernardoagbranco/react-threejs/info-page-2/src/App.js",[],"/Users/bernardoagbranco/react-threejs/info-page-2/src/components/Threejs.js",["181","182"],"/Users/bernardoagbranco/react-threejs/info-page-2/src/components/Info1/Info.js",[],"/Users/bernardoagbranco/react-threejs/info-page-2/src/components/Info2/Info.js",[],"/Users/bernardoagbranco/react-threejs/info-page-2/src/components/Info3/Info.js",[],"/Users/bernardoagbranco/react-threejs/info-page-2/src/components/Gallery/Gallery.js",[],"/Users/bernardoagbranco/react-threejs/info-page-2 - cópia/src/index.js",[],["183","184"],"/Users/bernardoagbranco/react-threejs/info-page-2 - cópia/src/App.js",[],"/U